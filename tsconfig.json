{
    "compilerOptions": {
        "outDir": "./spyvsspy/dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "declaration": true,
        "removeComments": true,
        "jsx": "react",
        "sourceRoot": "../spyvsspy/src/**/*.ts"
    },
    "include": [
        "./spyvsspy/src/**/*"
    ],
    "exclude": [
        "./node_modules/**/*"
    ]
}